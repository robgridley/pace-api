{
    "name": "robgridley/pace-api",
    "description": "EFI Pace API Client",
    "authors": [
        {
            "name": "Rob Gridley",
            "email": "me@robgridley.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-fileinfo": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "nesbot/carbon": "^2.0|^3.0",
        "doctrine/inflector": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|11.5|12.5|^13.0",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^6.4|^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pace\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pace\\PaceServiceProvider"
            ],
            "aliases": {
                "Pace": "Pace\\Facades\\Pace"
            }
        }
    }
}
